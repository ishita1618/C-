#include<iostream>
using namespace std;
int main(){
//outputting and math operators

cout << 5;  // to print integer value
cout << "Hey!"; //to print textual content
cout << 7+10;  //addition

//to print outputs in seperate lines
cout << 3+4;
cout << endl;
cout << 5+9;

//Area and Perimeter of rectangle taking length 10  breadth 8
cout << 10*8;endl;
cout << 2*(10+8);

//multiple outputs in cout
cout << 7+8 << endl << 9+3 ;

//inserting space and text between output
cout << 5+6 << " " << 5+4 ;
cout << 4-3 << "and" << 7+8 ;

//multiplication
cout << 4*3 << endl << 2 * (3+4) ;

//division
cout << 3*4 << endl << 10/5 ;

return 0;
}
